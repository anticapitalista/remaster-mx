#: /home/jbowlin/Projects/LiveUSB/v3/Test-1/xtra/usr/local/bin/toram-eject:14
msgid "\\n    Eject LiveCD/DVD via a simple GUI if %s is enabled."
msgstr ""
"\\n Uitwerpen van een LiveCD/DVD via een eenvoudige GUI als %s is ingesteld."

#: /home/jbowlin/Projects/LiveUSB/v3/Test-1/xtra/usr/local/bin/toram-eject:16
msgid "Automatic toram Eject"
msgstr "Automatisch uitwerpen met Toram"

#: /home/jbowlin/Projects/LiveUSB/v3/Test-1/xtra/usr/local/bin/toram-eject:24
msgid "no BOOT_DEV given!"
msgstr "geen BOOT_DEV is opgegeven!"

#: /home/jbowlin/Projects/LiveUSB/v3/Test-1/xtra/usr/local/bin/toram-eject:29
msgid "boot device: %s is not removable."
msgstr "boot device: %s is niet verwijderbaar."

#: /home/jbowlin/Projects/LiveUSB/v3/Test-1/xtra/usr/local/bin/toram-eject:33
msgid "boot device: %s is still mounted!"
msgstr "boot device: %s is nog aangekoppeld!"

#: /home/jbowlin/Projects/LiveUSB/v3/Test-1/xtra/usr/local/bin/toram-eject:40
msgid "The %s boot option succeeded."
msgstr "De %s boot-keuze is geslaagd."

#: /home/jbowlin/Projects/LiveUSB/v3/Test-1/xtra/usr/local/bin/toram-eject:41
msgid "The boot media is no longer needed."
msgstr "De boot media zijn niet langer vereist."

#: /home/jbowlin/Projects/LiveUSB/v3/Test-1/xtra/usr/local/bin/toram-eject:43
msgid "Do you want the LiveCD/DVD ejected?"
msgstr "Wilt u de LiveCD/DVD uitwerpen?"

#: /home/jbowlin/Projects/LiveUSB/v3/Test-1/xtra/usr/local/bin/toram-eject:44
msgid "did not eject as per user request."
msgstr "Het is niet uitgeworpen zoals gebruiker verzocht."

#: /home/jbowlin/Projects/LiveUSB/v3/Test-1/xtra/usr/local/bin/toram-eject:46
msgid "Could not eject %s"
msgstr "Kon %s niet uitwerpen"

#: /home/jbowlin/Projects/LiveUSB/v3/Test-1/xtra/usr/local/bin/toram-eject:51
msgid "Feel free to remove the CD or DVD."
msgstr "U kunt de CD of DVD nu vrijelijk verwijderen."

#: /home/jbowlin/Projects/LiveUSB/v3/Test-1/xtra/usr/local/bin/toram-eject:53
msgid "Do you want the drive closed now?"
msgstr "Wilt u de drive nu sluiten?"

#: /home/jbowlin/Projects/LiveUSB/v3/Test-1/xtra/usr/local/bin/toram-eject:54
msgid "did not closing drive as per user request."
msgstr "Kon de drive niet sluiten zoals gebruiker verzocht."

#: /home/jbowlin/Projects/LiveUSB/v3/Test-1/xtra/usr/local/bin/toram-eject:56
msgid "Could not close %s"
msgstr "Kon %s niet sluiten"
